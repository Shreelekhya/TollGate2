<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="false">
    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="50dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton"
                                        android:layout_row="0"
                                        android:layout_column="0"
                                        android:onClick="planYourtrip"
                                        android:background="#CD5C5C"

                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/car3"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="Plan Your
         Trip"
                                        android:layout_marginLeft="30dp"
                                        android:layout_marginBottom="5dp"
                                        android:textAlignment="center"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="50dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton1"
                                        android:onClick="tollpay"
                                        android:layout_row="1"
                                        android:layout_column="0"
                                        android:background="#CB4335"
                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/rupee1"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="Toll Pay"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="50dp"
                                        android:layout_marginBottom="25dp"
                                        android:textAlignment="center">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_weight="1"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton2"
                                        android:background="#CB4335"
                                        android:onClick="tollwallet"
                                        android:layout_row="2"
                                        android:layout_column="0"
                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/wallet1"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="Toll Wallet"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="25dp"
                                        android:layout_marginBottom="25dp"
                                        android:textAlignment="center">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton3"
                                        android:onClick="thist"
                                        android:layout_row="0"
                                        android:layout_column="1"
                                        android:background="#CD5C5C"
                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/thistory1"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="Transaction
        History"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="30dp"
                                        android:layout_marginBottom="10dp"
                                        android:textAlignment="center">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>

                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton4"
                                        android:layout_row="1"
                                        android:onClick="profile"
                                        android:layout_column="1"
                                        android:background="#CD5C5C"
                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/profile2"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="Profile"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="50dp"
                                        android:layout_marginBottom="35dp"
                                        android:textAlignment="center">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:padding="0dip"
                                android:layout_height="fill_parent">

                                <FrameLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageButton
                                        android:layout_width="195dp"
                                        android:layout_height="180dp"
                                        android:id="@+id/imageButton5"
                                        android:onClick="aboutus"
                                        android:layout_row="2"
                                        android:layout_column="1"
                                        android:background="#CB4335"
                                        />
                                    <ImageView android:layout_width="73dp"
                                        android:layout_height="73dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/abtus1"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:clickable="false"
                                        android:text="About Us"
                                        android:textColor="#F8F8FF"
                                        android:textSize="25dp"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="40dp"
                                        android:layout_marginBottom="35dp"
                                        android:textAlignment="center">
                                    </TextView>
                                </FrameLayout>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</ScrollView>